<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="portBridge" type="PortBridge.PortBridgeSection, PortBridge" />
  </configSections>
  <!-- Add your .NET Services project account information here -->
  <!-- Create a project at http://portal.ex.azure.microsoft.com/ -->
  <portBridge serviceBusNamespace="mysolution" serviceBusIssuerName="owner" serviceBusIssuerSecret="xxxxxxxx">
    <hostMappings>
      <add targetHost="localhost" allowedPorts="3389" />
    </hostMappings>
  </portBridge>
  <system.net>
    <defaultProxy enabled="true" />
  </system.net>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>